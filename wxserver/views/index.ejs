      <div class="col-lg-6 ds" id="contentId">
		      <h3>微信墙</h3>
			<div class="desc" ng-repeat="msg in msgs">
        <div class="thumb">
          <p><muted>{{msg.title}}</muted><br/></p>
          <p><muted>{{msg.author}}</muted><br/></p>
          <!-- <img src="{{msg.headimgurl}}" class="img-circle" width="35"> -->

        </div>
        <div class="details">
            <p><muted>{{msg.time}}</muted><br/>
                <a href="#">{{msg.href}}</a>{{msg.content}}
              	<br/>
            </p>
        </div>
      </div>



          </section>
      </section>

      <!--main content end-->

  </section>

